<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:ViscoTestApp.Resources">

	<Style x:Key="CoolShape" TargetType="Shape">

		<Setter Property="Stroke" Value="{StaticResource BalticSea}"></Setter>

		<Style.Triggers>
			<Trigger Property="IsMouseOver" Value="True">
				<Setter Property="Stroke" Value="{StaticResource SuvaGrey}"></Setter>
			</Trigger>
		</Style.Triggers>
	</Style>

</ResourceDictionary>